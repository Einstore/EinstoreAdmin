{
	"name": "einstore-admin",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"prod": "npm run build && npm run serve",
		"start": "cross-env BROWSER=none react-scripts start",
		"start-demo": "cross-env REACT_APP_API_URL=https://demo.einstore.io/api npm run start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"test:ci": "npm run test -- --coverage",
		"serve": "./node_modules/.bin/serve -l tcp://0.0.0.0:3000 -s build",
		"ci": "cross-env CI=true npm run build && npm run test:ci",
		"ci:run": "npm run ci && npm run serve",
		"format": "prettier --write \"src/**/*.{j,t}s{,x}\""
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "5.15.4",
		"@reach/router": "1.3.4",
		"@sentry/browser": "6.15.0",
		"babel-preset-react-app": "10.0.0",
		"classnames": "2.3.1",
		"color": "4.0.1",
		"cross-env": "7.0.3",
		"crypto-js": "4.1.1",
		"formik": "2.2.9",
		"fstream": ">=1.0.12",
		"get-best-contrast-color": "0.3.1",
		"jwt-decode": "3.1.2",
		"lodash-es": "4.17.21",
		"md5": "2.3.0",
		"memoize-one": "6.0.0",
		"node-sass": "6.0.1",
		"pretty-bytes": "5.6.0",
		"prop-types": "^15.7.2",
		"query-string": "7.0.1",
		"react": "17.0.2",
		"react-autosuggest": "10.1.0",
		"react-copy-to-clipboard": "5.0.4",
		"react-datalist": "^4.0.0",
		"react-dom": "17.0.2",
		"react-dropzone": "11.4.2",
		"react-pose": "4.0.10",
		"react-router": "6.0.2",
		"react-router-dom": "6.0.2",
		"react-scripts": "4.0.3",
		"react-select": "5.2.1",
		"serve": "13.0.2",
		"slugify": "1.6.2",
		"ts-node": "10.4.0",
		"tslint": "6.1.3",
		"typescript": "4.5.2",
		"whatwg-fetch": "3.6.2",
		"yup": "0.32.11"
	},
	"devDependencies": {
		"@types/classnames": "2.3.1",
		"@types/color": "3.0.2",
		"@types/crypto-js": "4.0.2",
		"@types/jest": "27.0.3",
		"@types/lodash-es": "4.17.5",
		"@types/memoize-one": "5.1.2",
		"@types/node": "14.17.34",
		"@types/pretty-bytes": "5.2.0",
		"@types/query-string": "^6.3.0",
		"@types/reach__router": "1.3.9",
		"@types/react": "17.0.35",
		"@types/react-copy-to-clipboard": "5.0.2",
		"@types/react-dom": "17.0.11",
		"@types/react-dropzone": "5.1.0",
		"@types/react-select": "5.0.1",
		"@types/yup": "0.29.13",
		"prettier": "2.4.1"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
